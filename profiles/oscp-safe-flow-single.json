{
	"profile_name": "OSCP Safe Flow - Single User",
	"target_enabled": true,
	"target_url": "http://TARGET/login",
	"target_method": "POST",
	"user_param": "username",
	"pass_param": "password",
	"extra_params": "",
	"success_codes": "200,302",
	"success_regex": "Welcome|Dashboard",
	"failure_regex": "Invalid|failed|try again",
	"success_require_redirect": false,
	"success_length_gt": 0,
	"qps": 1.0,
	"concurrency": 2,
	"headers_json": "{\n  \"Content-Type\": \"application/x-www-form-urlencoded\"\n}",
	"cookies": "",
	"proxy_url": "",
	"verify_tls": true,
	"timeout": 15,
	"async_engine": false,
	"http2": false,
	"max_connections": 10,
	"retries": 1,
	"backoff": 0.2,
	"sql_checks": false,
	"body_mode": "params",
	"raw_body": "",
	"username_value": "",
	"use_usernames_file": false,
	"usernames_file": "",
	"lowercase_usernames": true,
	"generate_email_patterns": false,
	"generate_aliases": false,
	"passwords_file": "./passwords.txt",
	"spray_enabled": false,
	"spray_cooldown_sec": 60,
	"spray_window_attempts": 3,
	"lockout_codes": "429,401",
	"lockout_regex": "locked|too many|captcha",
	"lockout_cooldown_sec": 300,
	"lockout_jitter": true,
	"tor_mode": false,
	"proxy_list_file": "",
	"proxy_rotation": "per_request",
	"ua_rotation": "builtin",
	"ua_list_file": "",
	"capture_enabled": true,
	"capture_max": 10,
	"capture_max_bytes": 65536,
	"capture_failures_only": true,
	"log_to_file": false,
	"log_file_path": "",
	"log_append": true,
	"enable_checkpoint": true,
	"checkpoint_file": "./checkpoint.json",
	"resume_checkpoint": false,
	"auto_form": true,
	"refresh_csrf_each": true,
	"prelogin_enabled": false,
	"prelogin_per_attempt": false,
	"prelogin_urls": "",
	"prelogin_headless_js": false,
	"safe_auto_pause": true,
	"flow_enabled": true,
	"flow_per_attempt": true,
	"flow_steps": [
		{
			"method": "GET",
			"url": "http://TARGET/login",
			"headers": {},
			"body_mode": "params",
			"body": "",
			"extract_regex": "name=\"csrf\" value=\"([^\"]+)\"",
			"extract_var": "csrf"
		},
		{
			"method": "POST",
			"url": "http://TARGET/login",
			"headers": {"Content-Type": "application/x-www-form-urlencoded"},
			"body_mode": "raw",
			"body": "username={{username}}&password={{password}}&csrf={{csrf}}",
			"extract_regex": "",
			"extract_var": ""
		}
	],
	"notes": "Enter the single username in the Username field. Replace TARGET with the host/IP. Provide passwords.txt. Safe Mode caps QPS and concurrency and auto-pauses on lockout signals."
}